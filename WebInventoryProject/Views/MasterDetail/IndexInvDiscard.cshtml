@model IEnumerable<WebInventoryProject.Models.invDiscardMaster>

@{
    ViewBag.Title = "IndexInvDiscard";
    Layout = "~/Views/Shared/_LayoutAdminPanel.cshtml";
}
</br>
</br>
</br>


<h2>IndexInvDiscard</h2>
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <link rel="stylesheet" type="text/css" href="//cdn.datatables.net/1.10.25/css/jquery.dataTables.min.css" />
    <script type="text/javascript" src="//cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#myTable').dataTable();
        });
    </script>
</head>
<body>

    <div>
        <a style="margin-right:30px" class="btn btn-primary pull-right" href="@Url.Action("InvDiscardMasterDetail","MasterDetail")"><i class="fa fa-plus"></i>Add Discard</a>
    </div>


    <div id="page-wrapper">
        <div class="main-page login-page " style="width:90%;margin-top:-25px;">
            <div class="widget-shadow">
                <div class="login-body bg-light">
                    @Html.Partial("MessageAlert")
                    <h2 class="title1">Discard Master Detail</h2>
                    <br />
                    <div>
                        @*Receive All Database Data From Controller And Display Those Data In Client Side*@

                        <div class="table_bordered" width="600px;">
                            <table class="table table-striped table-bordered" id="myTable">
                                <thead>
                                    <tr>
                                      
                                        <th>
                                            Br Code
                                        </th>
                                        <th>
                                          Dept Code
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.discardDate)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.discardNo)
                                        </th>
                                      
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                    <tr>
                                       
                                        <td>
                                            @Html.DisplayFor(modelItem => item.settingBranch.branchCode)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.settingDepartment.departmentCode)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.discardDate)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.discardNo)
                                        </td>
                                     
                                        <td>
                                            <button class="btn btn-xs btn-default">@Html.ActionLink("View Details", "InvDiscardDetails", new { discardId = item.discardId }, new { Style = "color:White" })</button>   |

                                        </td>
                                    </tr>
                                    }

                                </tbody>
                            </table>
                        </div>



                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>




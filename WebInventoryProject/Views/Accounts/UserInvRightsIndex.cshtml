@model IEnumerable<WebInventoryProject.Models.settingUserInventoryRights>

@{
    ViewBag.Title = "UserInvRightsIndex";
    Layout = "~/Views/Shared/_LayoutAdminPanel.cshtml";
}

<h2>Inventory User Rights</h2>




<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <link rel="stylesheet" type="text/css" href="//cdn.datatables.net/1.10.25/css/jquery.dataTables.min.css" />
    <script type="text/javascript" src="//cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript">
            $(document).ready(function () {
                $('#myTable').dataTable();
            });
    </script>
</head>
<body>
    <div class="table_bordered" width="600px;">
        <div id="page-wrapper">
            <div class="main-page login-page " style="width:90%;">
                <div class="widget-shadow">
                    <div class="login-body">
                        @Html.Partial("MessageAlert")
                        <h2 class="title1">Branches</h2>
                        <br />
                        <div class="pull-right">
                            <a class="btn btn-success" style="margin-bottom:10px" href="@Url.Action("Branch","Branch")"><i class="fa fa-plus"></i>ADD</a>
                        </div>

                        <div class="table_bordered" width="600px;">
                            <table class="table table-striped table-bordered" id="myTable">
                                <thead style="background:lightgreen;">
                                    <tr>
                                        <th>
                                            @Html.DisplayNameFor(model => model.loginUser.email)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.settingBranch.branchCode)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.formId)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.save)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.edit)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.delete)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.post)
                                        </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.loginUser.email)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.settingBranch.branchCode)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.formId)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.save)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.edit)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.delete)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.post)
                                        </td>
                                        <td>
                                            <button class="btn btn-xs btn-default"><i class="fa fa-pencil"> </i>@Html.ActionLink("Edit", "UserInventoryRights", new { Id = item.Id }, new { Style = "color:White" })</button>   |
                                            <button class="btn btn-xs btn-danger"><i class="fa fa-trash"></i> @Html.ActionLink("Delete", "DeleteUserInvRights", new { Id = item.Id }, new { Style = "color:White" })</button>   |
                                        </td>
                                    </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>

<table class="table">
   

   

</table>